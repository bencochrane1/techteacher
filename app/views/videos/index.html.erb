
    <div class="center">
  <div class="row">
    <% @videos.each do |video| %>
    <div class="col-md-3">
      <div class="thumbnail">
        <%= image_tag href="/img/video.jpg" %>
          <div class="caption">
            <h3><%= video.title %></h3>
              <p><%= video.description %></p>
          </div>
      </div>  
    </div>
    <% end %>
  </div>
</div>