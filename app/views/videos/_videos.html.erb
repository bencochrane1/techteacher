
    <% @videos.each do |video| %>
    <div class="col-md-3">
      <div class="thumbnail">
        <%= image_tag(video.categories.first.image.url) %>
          <div class="caption">
            <h3><%= link_to video.title, video %></h3>
              <p><%= video.description %></p>
          </div>
      </div>  
    </div>
    <% end %>
  </div>